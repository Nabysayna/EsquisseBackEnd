<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Departements" table="departements">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="departement" type="string" column="departement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="latitude" type="integer" column="latitude" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="longitude" type="integer" column="longitude" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idregion" type="integer" column="idregion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="region" type="string" column="region" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
