<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Charge" table="charge">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idgerantpdv" type="integer" column="idgerantpdv" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idadminpdv" type="integer" column="idadminpdv" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="libelle" type="string" column="libelle" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="montant" type="bigint" column="montant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
