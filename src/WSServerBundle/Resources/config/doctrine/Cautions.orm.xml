<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Cautions" table="cautions">
    <id name="idCaution" type="integer" column="id_caution">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="caution" type="integer" column="caution" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cautionconsomme" type="integer" column="cautionconsomme" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAjout" type="datetime" column="date_ajout" nullable="false"/>
    <field name="dateModif" type="datetime" column="date_modif" nullable="true"/>
  </entity>
</doctrine-mapping>
