<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Commandes" table="commandes">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idArticle" type="integer" column="id_article" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commanditaire" type="integer" column="commanditaire" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pourvoyeur" type="integer" column="pourvoyeur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fullname" type="string" column="fullname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tel" type="bigint" column="tel" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateCommande" type="datetime" column="date_commande" nullable="false"/>
    <field name="etat" type="integer" column="etat" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
