<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Clients" table="clients">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="prenom" type="string" column="prenom" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="string" column="telephone" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fullname" type="string" column="fullname" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="genre" type="string" column="genre" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="age" type="boolean" column="age" nullable="true"/>
    <field name="adresse" type="string" column="adresse" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateAjout" type="datetime" column="date_ajout" nullable="false"/>
    <field name="nbreOperation" type="integer" column="nbre_operation" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="fidelite" type="integer" column="fidelite" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dependsOn" type="integer" column="depends_on" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
