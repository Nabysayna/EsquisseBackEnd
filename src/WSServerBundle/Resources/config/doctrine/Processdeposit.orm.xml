<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Processdeposit" table="processdeposit">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idadminpdv" type="integer" column="idadminpdv" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="montantdeposit" type="integer" column="montantdeposit" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etatdemande" type="smallint" column="etatdemande" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="datedemande" type="datetime" column="datedemande" nullable="false"/>
    <field name="initiateur" type="string" column="initiateur" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="agentRecouvreur" type="string" column="agent_recouvreur" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imageqrcode" type="string" column="imageqrcode" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="agentPositionneur" type="string" column="agent_positionneur" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
