<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Fidelisation" table="fidelisation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typeOperation" type="string" column="type_operation" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="montant" type="integer" column="montant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nbrePoint" type="integer" column="nbre_point" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
