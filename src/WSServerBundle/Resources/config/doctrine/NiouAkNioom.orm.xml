<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\NiouAkNioom" table="niou_ak_nioom">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="api" type="integer" column="api" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pwd" type="string" column="pwd" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deposit" type="integer" column="deposit" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
