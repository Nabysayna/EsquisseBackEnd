<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Services" table="services">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idadminpdv" type="integer" column="idadminpdv" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idpdv" type="integer" column="idpdv" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="designations" type="string" column="designations" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateAjout" type="datetime" column="date_ajout" nullable="false"/>
  </entity>
</doctrine-mapping>
