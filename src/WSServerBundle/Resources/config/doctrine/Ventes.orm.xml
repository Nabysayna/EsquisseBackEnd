<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Ventes" table="ventes">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCommande" type="integer" column="id_commande" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="montant" type="integer" column="montant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateVente" type="datetime" column="date_vente" nullable="false"/>
    <field name="telClient" type="bigint" column="tel_client" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresseVendeur" type="string" column="adresse_vendeur" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresseFournisseur" type="string" column="adresse_fournisseur" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
