<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Westernunion" table="westernunion">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="iduser" type="integer" column="idUser" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="typeoperation" type="string" column="typeOperation" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="infosoperation" type="string" column="infosOperation" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateOperation" type="datetime" column="date_operation" nullable="false"/>
  </entity>
</doctrine-mapping>
