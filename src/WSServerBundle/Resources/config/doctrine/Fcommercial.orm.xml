<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Fcommercial" table="fcommercial">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCommercial" type="integer" column="id_commercial" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="telclient" type="integer" column="telclient" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="prenomclient" type="string" column="prenomclient" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomclient" type="string" column="nomclient" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categorie" type="string" column="categorie" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
