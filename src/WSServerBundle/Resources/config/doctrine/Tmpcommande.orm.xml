<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Tmpcommande" table="tmpcommande">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idArticle" type="integer" column="id_article" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="qte" type="integer" column="qte" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateCommande" type="datetime" column="date_commande" nullable="false"/>
    <field name="idClient" type="integer" column="id_client" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codeCommande" type="integer" column="code_commande" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
