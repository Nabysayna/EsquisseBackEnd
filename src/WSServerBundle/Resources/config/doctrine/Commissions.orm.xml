<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WSServerBundle\Entity\Commissions" table="commissions">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomservice" type="string" column="nomservice" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="libelleoperation" type="string" column="libelleoperation" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="montant" type="integer" column="montant" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="frais" type="integer" column="frais" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="comConcedant" type="integer" column="com_concedant" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="comDistributeur" type="integer" column="com_distributeur" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commissionbbs" type="integer" column="commissionbbs" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commissionpdv" type="integer" column="commissionpdv" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateoperation" type="datetime" column="dateoperation" nullable="true"/>
  </entity>
</doctrine-mapping>
